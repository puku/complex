{
    "AWSEBDockerRunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"puku/complex-client",
            "hostname":"client",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"puku/complex-nginx",
            "essential":true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":[
                "client",
                "server"
            ]
        },
        {
            "name":"server",
            "image":"puku/complex-server",
            "hostname":"server",
            "essential":false
        },
        {
            "name":"worker",
            "image":"puku/complex-worker",
            "hostname":"worker",
            "essential":false
        }
    ]
}